{
  "name": "BroncoDirectMe",
  "description": "Test Description",
  "version": "1.0",
  "manifest_version": 3,
  "action": {
    "default_popup": "index.html",
    "default_icon": {
      "16": "/images/icon.png",
      "32": "/images/icon.png",
      "48": "/images/icon.png",
      "128": "/images/icon.png"
    }
  },
  "permissions": ["activeTab", "storage", "tabs", "identity", "identity.email"],
  "content_scripts": [
    {
      "js": ["content.js"],
      "matches": ["https://cmsweb.cms.cpp.edu/*"]
    }
  ]
}
