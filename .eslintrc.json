{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "plugin:react/recommended",
    "standard-with-typescript",
    "prettier",
    "plugin:jsdoc/recommended-typescript-error"
  ],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module",
    "project": ["tsconfig.json"]
  },
  "plugins": ["react"],
  "rules": {
    "@typescript-eslint/strict-boolean-expressions": 0,
    "require-jsdoc": 0
  },
  "settings": {
    "react": {
      "version": "detect"
    }
  }
}
